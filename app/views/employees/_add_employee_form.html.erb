<%= form_for([@company, @company.employees.build]) do |f| %>
	<p>
		First Name <%= f.text_field :first_name %>
	</p>
	<p>
		Last Name <%= f.text_field :last_name %>
	</p>
	<p>
		Email <%= f.text_field :email %>
	</p>
	<p>
		Job Title <%= f.text_field :job_title %>
	</p>
	<p>
		<%= f.submit %>
	</p>
<% end %>